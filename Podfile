# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'MYIOSDEV' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MYIOSDEV    
  # Connect with network http (internet required) https://cocoapods.org/pods/Alamofire
  pod 'Alamofire', '~> 4.7.3'

  # Loading image file.
  pod 'AlamofireImage', '~> 3.4.1'

  # Open Youtube
  pod 'XCDYouTubeKit', '~> 2.6.0'

  # SQLite Manager app
  pod 'FMDB', '~> 2.7.5'

  # QRCode Reader
  pod 'QRCodeReader.swift', '~> 9.0.1'

  # QRCode Generator
  pod 'QRCode', '~> 2.0'

  # Charts Generator
  pod 'Charts', '~> 3.2.0'

end
